CREATE database `ser_d1` charset 'latin1';
-> { type: "database-create", database: "ser_d1", charset: "latin1", if-not-exists: false }

CREATE database if not exists `ser_d2`;
-> { type: "database-create", database: "ser_d2", charset: null, if-not-exists: true }

ALTER database `ser_d1` charset 'utf8'
-> { type: "database-alter", database: "ser_d1", charset: "utf8" }

DROP database `ser_d2`;
-> { type: "database-drop", database: "ser_d2", if-exists: false }

DROP database if exists `ser_d1`;
-> { type: "database-drop", database: "ser_d1", if-exists: true }

